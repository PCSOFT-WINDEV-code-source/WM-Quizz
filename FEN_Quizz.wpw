      // Résumé : Enregistre le score du quizz
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE EnregistreScore()
      
      // Niveau
      Score.Niveau = gnNiveau
      
      // calcul du score
      SI gnNbBonnesRéponses > NB_QUESTION ALORS
      	gnNbBonnesRéponses = NB_QUESTION
      FIN
      Score.Valeur = gnNbBonnesRéponses
      
      Score.Temps = gnTempsEcouléTotal
      
      // Date heure
      dhMaintenant est une dateheure
      Score.DateHeure = dhMaintenant
      
      // Ajout du score
      SI HAjoute(Score) ALORS
      	
      SINON
      	Erreur(HErreurInfo() )
      FIN
