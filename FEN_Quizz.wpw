   -
     name : ValideRéponse
     procedure_id : 2190888785081601534
     type_code : 14
     code : |1+
      // Résumé : Procédure appelée par l'appui d'un bouton
      // Paramètres :
      //	sRéponse : Texte de la réponse validée par l'utilisateur
      //	nIndiceRéponse : Indice de la réponse validée par l'utilisateur (compris entre 1 et 3)
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ValideRéponse(sRéponse, nIndiceRéponse)
      
      SI gbActif ALORS
      	bBonneRéponse est un booléen = faux
      	
      	// fin du chrono
      	gnTempsEcouléTotal += ChronoFin(1)
      	
      	// on arrête le chrono si on est en mode stress
      	SI gbModeStress ALORS
      		// Arrêt du chrono
      		ArrêteChrono()
      	FIN
      	
      	SI ChaîneCompare(gsBonneRéponse,sRéponse) = 0 ALORS
      		// C'est la bonne réponse
      		bBonneRéponse = Vrai
      		gnNbBonnesRéponses ++
      	FIN
      	
      	SI  PAS gbModeStress ALORS
      		
      		AfficheBonneRéponse(bBonneRéponse,nIndiceRéponse)
      	
      	SINON
      		// On affiche la bonne réponse
      		AfficheBonneRéponse(bBonneRéponse,nIndiceRéponse,vrai)
      		
      		// on attend 1 seconde
      		<COMPILE SI TypeConfiguration=Android OU TypeConfiguration=iOS>
      			Multitâche(-100)
      		<FIN>
      		<COMPILE SI TypeConfiguration=UniversalWindowsApp>
      			ThreadPause(100)
      		<FIN>
      		
      		
      		// Active les boutons
      		POUR i = 1 _A_ NB_REPONSE
      			{"BTN_REPONSE" + i, indChamp}..Etat = Actif
      		FIN
      		
      		// On re active le programme
      		gbActif = Vrai
      		
      		JAUGE_Temps..Visible=faux
      		
      		// On passe à la suite
      		GénèreQuestion()
      	FIN
      FIN
      
     type : 458752
