   -
     name : QuestionDéjàPosée
     internal_properties : CAAAAAgAAABYapxWf6SQiTvGaoDf7l25bHYHeLbl9QWZGgTPp7deAoi3IcQG6Q1DD+/4LLFOnNZlDHx5/xgxQwJsG3bZPPwmSwmFpZDJzP2VCqisPq0No3Unnr69tLB8p/g9WpGYlbNgYhqAZL0oJMGIvPQlB/NHgRIfmCMQHnDOZyiZnoT1XRiXoDIwm40rupv3wtOJ7UGFyQjFfQ==
     procedure_id : 2190851405967950104
     type_code : 14
     code : |1-
      // Résumé : Est ce qu'on a déjà posé la question?
      // Paramètres :
      //	nIDQuestion : L'id de la question que l'on veut posée
      // Valeur de retour :
      // 	Type indéterminé : Vrai si cette question a déjà été posée
      //
      PROCEDURE QuestionDéjàPosée(nIDQuestion)
      
      SI TableauOccurrence(gtabQuestionsDéjàPosées) = gnNbQuestions ALORS
      	renvoyer -1
      FIN
      
      Si TableauCherche(gtabQuestionsDéjàPosées,tclinéaire,nIDQuestion) <> -1 ALORS
      	renvoyer vrai
      sinon
      	renvoyer faux
      FIN
     type : 458752
   -
     name : AfficheQuestion
     procedure_id : 2190857204173838783
     type_code : 14
     code : |1-
      // Résumé : Affiche la question de l'enregistrement en cours
      // Paramètres :
      //	nIDQuestion :ID de la question de l'enregistrement en cours
      //	nNiveauQuestion :Niveau de la question en cours
      //	sTexte (chaîne ANSI) :Texte de la question en cours
      //	nTypeRéponse (entier) :Type de la réponse, TYP_QCM ou TYPE_SAISIE
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheQuestion(LOCAL nIDQuestion, LOCAL nNiveauQuestion, LOCAL sTexte est une chaine, LOCAL nTypeRéponse est un entier)
      
      // Contenu de la question
      sQuestion est une chaine = sTexte
      Si  ChaîneOccurrence(sQuestion,"?") = 0 ALORS
      	sQuestion += " ?"
      FIN
      
      // On remplace " ?" avec un espace insécable pour éviter les retours à la ligne juste avec un "?"
      sQuestion = Remplace(sQuestion," ?",CaractUnicode(0160)+"?") 
      
      LIB_Question = sQuestion
      
      
      // affichage
      Visible_GR_BonneReponse(faux)
      
      POUR i = 1 _a_ NB_REPONSE
      	{"BTN_REPONSE" + i, indChamp}..CouleurFond = IMG_Réponse_Normal..CouleurFond
      FIN
      
      // Numéro de la question
      gnQuestionEnCours++
      LIB_Num_Question = gnQuestionEnCours + "/10"
      
      
      // Ajout dans le tableau
      TableauAjoute(gtabQuestionsDéjàPosées,nIDQuestion)
      
      // Réponses possibles
      Si nTypeRéponse = TYPE_QCM  ALORS
      	// On remplit le sélecteur avec les réponses
      	RemplisSélecteur(nIDQuestion,nNiveauQuestion)
      FIN
      
      Visible_GR_BonneReponse(Faux)
      
      // Démarrage du chrono
      ChronoDébut(1)
      // Mode stress
      Si gbModeStress ALORS
      	DémarreChrono()
      FIN
     type : 458752
   -
     name : RemplisSélecteur
     procedure_id : 2190859201333703854
     type_code : 14
     code : |1+
      // Résumé : Remplit le sélecteur pour les réponse
      // Paramètres :
      //	nIDQuestion :L'ID de la question
      //	nNiveauQuestion :Niveau de la question
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplisSélecteur(LOCAL nIDQuestion, LOCAL nNiveauQuestion)
      
      // Variables locales
      nIndice est un entier = 1
      
      // masque la dernière au cas ou il y en ai que 2
      BTN_REPONSE3..visible = faux
      
      // Lecture des réponses
      HLitRecherchePremier(Reponse,IDQuestion,nIDQuestion)
      TANTQUE HTrouve(Reponse)
      	// 3 réponse max
      	SI nIndice > NB_REPONSE ALORS
      		SORTIR
      	FIN
      	
      	SI nNiveauQuestion <> NIV_MIXT _OU_ Reponse.Niveau = gnNiveau ALORS
      		SI nIndice = 3 ALORS
      			BTN_REPONSE3..Visible = Vrai
      		FIN
      		
      		// Affectation
      		{"BTN_REPONSE" + nIndice,indChamp}..Libellé = Reponse.Texte
      		
      		// Bonne réponse
      		SI Reponse.Bonne ALORS
      			gsBonneRéponse = Reponse.Texte
      			gnIndiceBonneRéponse = nIndice
      		FIN
      		
      		// Réponse suivante
      		nIndice++
      	FIN
      	
      	
      	HLitSuivant(Reponse)
      FIN
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
