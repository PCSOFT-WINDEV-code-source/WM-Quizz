   -
     name : GénèreQuestion
     internal_properties : CAAAAAgAAABO6arJCKQSIZoq17XqRqtSp+QELHqDZX7QKvRFpRx6a4YTsdSBckBtUDjq1f64+c5ikl6jXihNQzrmtWbU5gy5KZJ49BjQ75aACEKnj4SbyNk2lkhIHaJBz6Q/h6I3r44+pTaGrzAcx8vu3LvaVle6IwGFbJaDRt2GM1LMi5d1qBTuqByVdNtGIo+RlwqSqyXDDeOadR4=
     procedure_id : 2190849146815085249
     type_code : 14
     code : |1+
      // Résumé : Affiche une question au hasard
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE GénèreQuestion()
      
      // Fin du quizz
      SI gnQuestionEnCours >= NB_QUESTION ALORS
      	TermineQuizz()
      	retour
      FIN
      
      // Variables locales
      nHasard est un entier
      nIndice est un entier = 1
      
      // Création du hasard
      nHasard = Hasard(1,gnNbQuestions)
      
      // Parcours des questions
      HLitPremier(Question,gsCléParcours)
      TANTQUE PAS HEnDehors()
      	SI PAS gbModeStress _OU_ Question.TypeRéponse<>TYPE_SAISIE ALORS
      		// Est ce que c'est la question voulue
      		SI nIndice = nHasard ALORS
      			// Est ce qu'on ne l'a pas déjà posé?
      			SELON QuestionDéjàPosée(Question.IDQuestion)
      				CAS Vrai
      					// Initialisation du hasard
      					InitHasard()
      					
      					// On rappelle cette même méthode
      					GénèreQuestion()
      				CAS Faux
      					// Explication de la réponse
      					gsExplication = Question.Explication
      					gnTypeQuestionEnCours = Question.TypeRéponse
      					gsBonneRéponse = ""
      					gnIndiceBonneRéponse = 0
      					
      					// Affichage de la question
      					AfficheQuestion(Question.IDQuestion,Question.Niveau,Question.Texte,Question.TypeRéponse)
      				AUTRE CAS
      					TermineQuizz()
      			FIN
      			
      			SORTIR
      		FIN
      		
      		// Passage à l'enregistrement suivant
      		nIndice++
      	FIN
      	
      	HLitSuivant(Question,gsCléParcours)
      FIN
      
      
      
      
      
      
      
      
      
      
