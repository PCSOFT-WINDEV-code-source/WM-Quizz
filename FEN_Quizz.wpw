      // Résumé : Démarre un chrono, utile pour le calcul du temps en mode stress
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DémarreChrono()
      
      // Réinitialise la jauge
      JAUGE_Temps..Valeur = 10000
      JAUGE_Temps..image = IMG_JAUGE_OK..CouleurFond
      
