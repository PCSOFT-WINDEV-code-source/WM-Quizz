      // Résumé : Démarre un Quizz
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DémarreQuizz()
      
      // Mode stress activé
      SI gbModeStress=Vrai ALORS
      	JAUGE_Temps..Visible  = Vrai
      FIN
      
      gnNbQuestions = 0
      
      // Initialisation du hasard
      InitHasard()
      
      // Filtre sur le niveau de la question
      gsCléParcours = HFiltre(Question,Niveau,gnNiveau)
      
      // Récupération du nombre de questions pour ce niveau
      HLitPremier(Question,gsCléParcours)
      
      TANTQUE HEnDehors(Question)=faux
      	SI PAS gbModeStress _OU_ Question.TypeRéponse <> TYPE_SAISIE ALORS
      		gnNbQuestions ++	
      	FIN
      	
      	HLitSuivant(Question,gsCléParcours)
      FIN
      
      // Par défaut les 10 questions ne sont pas encore répondus
      pour i =1 a 10
      	{"IMG_Question" + i, indChamp} = IMG_Réponse_normal..CouleurFond
      FIN
      
      // Génération d'une question
      SI gnNbQuestions > 0 ALORS
      	// Affiche une question
      	GénèreQuestion()
      SINON
      	Erreur("Aucune question disponible pour ce mode.")
      	Ferme()
      FIN
